#!/vsta/bin/sh
set -e
set -x
if test \( "$1" = "all" \) -o \( "$1" = "install" \) -o \( "$1" = "" \)
then
	cd libc
	make libusr.a
	cd ..
fi
cd os/make
make $1
cd ../..
cd libc
make $1
cd ..
cd srv
cd bfs
make $1
cd cmd
make $1
cd ..
cd ..
cd namer
make $1
cd ..
cd swap
make $1
cd ..
cd env
make $1
cd ..
cd dos
make $1
cd ..
cd tmpfs
make $1
cd ..
cd pipe
make $1
cd ..
cd vstafs
make $1
cd cmd
make $1
cd ..
cd ..
cd devnull
make $1
cd ..
cd cdfs
make $1
cd ..
cd proc
make $1
cd cmd
make $1
cd ..
cd ..
cd mach
cd cons2
make $1
cd ..
cd fd
make $1
cd ..
cd wd
make $1
cd ..
cd rs232
make $1
cd ..
cd scsi
make $1
cd ..
cd mouse
make $1
cd ..
cd par
make $1
cd ..
cd joystick
make $1
cd ..
cd nvram
make $1
cd ..
cd ne
make $1
cd ..
cd ..
cd ..
cd bin.src/test
make $1
cd ../login
make $1
cd ../init
make $1
cd ../testsh
make $1
cd ../time
make $1
cd ../roff
make $1
cd ../adb
make $1
cd ../..
exit 0
